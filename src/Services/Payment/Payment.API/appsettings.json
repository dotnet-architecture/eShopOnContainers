{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    }
  },
  "PaymentSucceeded": true,
  "AzureServiceBusEnabled": false,
  "SubscriptionClientName": "Payment",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "EventBusRetryCount": 5,
  "KafkaEnabled": true,
  "Kafka": {
    "ProducerSettings": {
      "BootstrapServers": "broker:9092",
      "AllowAutoCreateTopics": true
    },
    "ConsumerSettings": {
      "BootstrapServers": "broker:9092",
      "GroupId": "payment-group-id",
      "AllowAutoCreateTopics": true
    }
  }
}
